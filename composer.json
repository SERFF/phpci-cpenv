{
  "name": "serff/phpci-env",
  "description": "PHPCI plugin to copy env file",
  "keywords": [
    "phpci",
    "env",
    "laravel"
  ],
  "license": "MIT",
  "type": "library",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Serff\\PhpciEnv\\": "src/"
    }
  },
  "extra": {
    "phpci": {
      "pluginNamespace": "Serff\\PhpciEnv\\",
      "suppliedPlugins": [
        {
          "name": "CpEnv",
          "class": "CpEnv"
        }
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Serff\\PhpciEnv\\Tests\\": "tests/"
    }
  }
}
